.contain
{

    background-image: url("https://www.gxfwz36524.com/car/public/forune.jpg");
    background-size: 100% 100%;
    background-repeat: no-repeat;
    overflow-x: hidden;
}
.card
{
    width:300px;
    height: 300px;
    background-color:white;
    box-shadow:0px 5px 20px #999;
    top:40%;
    left:50%;
    transform: translate(-50%, -50%);
    position: absolute;
    overflow: hidden;

}